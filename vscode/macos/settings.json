  // Remove unnecessary extension notifications and telemetry
  "telemetry.telemetryLevel": "error",
  "extensions.ignoreRecommendations": true,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "redhat.telemetry.enabled": false,
  "shellcheck.disableVersionCheck": true,
  "workbench.activityBar.location": "top",
  "workbench.welcomePage.walkthroughs.openOnInstall": false,
  "workbench.editor.historyBasedLanguageDetection": true,
  // Experimental
  "workbench.editor.doubleClickTabToToggleEditorGroupSizes": "maximize",
  // Experimental
  "workbench.iconTheme": "vscode-icons",
  "errorLens.excludeByMessage": [
    "Expected 'this' to be used by class method",
    {
      "regex": "^Delete",
    },
    {
      "regex": "^MD", // Markdownlint
    },

    // Formatting related
    {
      "regex": "^Insert `",
    },
    {
      "regex": "^Replace `",
    },
    "Expected indentation",
    "Trailing spaces not allowed",
    "Block must not be padded",
    "Missing trailing comma",
    "Missing semicolon",
  ],
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "deno.codeLens.implementations": true,
  "journal.templates": [
    {
      "name": "entry",
      "template": "# ${d:dddd, MMMM DD YYYY}\n\n## Standup\n\n\n\n## Tasks\n\n\n\n## Notes\n\n"
    }
  ],
  "auto.languages": {
    "javascript": {
      "symbol": "$",
      "stringWrapper": "`"
    },
    "javascriptreact": {
      "symbol": "$",
      "stringWrapper": "`"
    },
    "typescript": {
      "symbol": "$",
      "stringWrapper": "`"
    },
    "typescriptreact": {
      "symbol": "$",
      "stringWrapper": "`"
    }
  },
  // Suppress trailing comma warnings in settings.json
  "json.schemas": [
    {
      "fileMatch": [
        "settings.json"
      ],
      "schema": {
        "allowTrailingCommas": true
      }
    }
  ],
