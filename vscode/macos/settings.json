  "multiCommand.commands": {
    "multiCommand.makeSpace": {
      "label": "Make Space",
      "sequence": [
        "workbench.action.toggleActivityBarVisibility",
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    "multiCommand.commentThenDuplicate": {
      "label": "Comment Then Duplicate",
      "sequence": [
        "editor.action.commentLine",
        "editor.action.copyLinesDownAction",
        "editor.action.commentLine"
      ]
    },
    "multiCommand.closeAllTabsExceptCurrent": {
      "label": "Close All Tabs Except Current",
      "sequence": [
        "workbench.action.closeEditorsInOtherGroups",
        "workbench.action.closeOtherEditors",
        "workbench.files.action.collapseExplorerFolders",
        "workbench.action.closePanel",
        "workbench.files.action.showActiveFileInExplorer"
      ]
    },
    "multiCommand.deletePairedBrackets": {
      "label": "Delete Paired Brackets",
      // removeBrackets, deleteLines, then format
      "sequence": [
        "editor.action.removeBrackets",
        "editor.action.deleteLines",
        "editor.action.formatDocument"
      ]
    }
  },
  "settings.cycle": [
    {
      "id": "screenshare",
      "overrideWorkspaceSettings": false,
      "values": [
        {
          "window.zoomLevel": 0.25,
          "editor.minimap.enabled": false
        },
        {
          "window.zoomLevel": -0.5,
          "editor.minimap.enabled": true
        },
        {
          "window.zoomLevel": -1.0,
          "editor.minimap.enabled": true
        }
      ]
    },
    {
      "id": "livestream",
      "overrideWorkspaceSettings": false,
      "values": [
        {
          "editor.cursorBlinking": "solid",
          "scm.diffDecorations": "none",
          "window.zoomLevel": 1,
          "workbench.activityBar.visible": false,
          "workbench.statusBar.visible": false
        },
        // My Defaults
        {
          "editor.cursorBlinking": "smooth",
          "editor.fontSize": 13,
          "scm.diffDecorations": "all",
          "terminal.integrated.fontSize": 11,
          "window.zoomLevel": -0.5,
          "workbench.activityBar.visible": true,
          "workbench.statusBar.visible": true
        }
      ]
    }
  ],
  "settingsOnFire.toggle": {
    "GitHub CoPilot": {
      "on": {
        "_label": "Enable",
        "editor.inlineSuggest.enabled": true
      },
      "off": {
        "_label": "Disable",
        "editor.inlineSuggest.enabled": false
      }
    },
    "Screen Share Mode": {
      "on": {
        "_label": "Enable",
        "window.zoomLevel": 1,
        "editor.minimap.enabled": false
      },
      "off": {
        "_label": "Disable",
        "window.zoomLevel": -0.25,
        "editor.minimap.enabled": true
      }
    },
    "Internals / Artifacts": {
      "on": {
        "_label": "Hide",
        "files.exclude": {
          "**/node_modules": true,
          "**/dist": true,
          "**/build": true,
          "**/cdk.out": true,
          "**/.turbo": true
        }
      },
      "off": {
        "_label": "Show",
        "files.exclude": {
          "**/node_modules": false,
          "**/dist": false,
          "**/build": false,
          "**/cdk.out": false,
          "**/.turbo": false
        }
      }
    }
  },
